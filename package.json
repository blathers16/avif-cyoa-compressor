{
  "name": "avif-cyoa-compressor",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run clean:assets && npm run copy:assets && ng serve",
    "build": "npm run clean:assets && npm run copy:assets && ng build",
    "watch": "npm run clean:assets && npm run copy:assets && ng build --watch",
    "serve": "npx http-server dist/avif-cyoa-compressor",
    "test": "npm run clean:assets && npm run copy:assets && ng test",
    "copy:assets": "globby-cp node_modules/**/*.wasm src/assets/wasm/",
    "clean:assets": "del-cli src/assets/wasm/*.wasm"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.0",
    "@angular/common": "^17.3.0",
    "@angular/compiler": "^17.3.0",
    "@angular/core": "^17.3.0",
    "@angular/forms": "^17.3.0",
    "@angular/platform-browser": "^17.3.0",
    "@angular/platform-browser-dynamic": "^17.3.0",
    "@angular/router": "^17.3.0",
    "@imagemagick/magick-wasm": "^0.0.28",
    "@jsquash/avif": "^1.3.0",
    "@ng-bootstrap/ng-bootstrap": "^16.0.0",
    "@popperjs/core": "^2.11.8",
    "base64-arraybuffer": "^1.0.2",
    "bootstrap": "^5.3.3",
    "observable-webworker": "^6.0.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.3",
    "@angular/cli": "^17.3.3",
    "@angular/compiler-cli": "^17.3.0",
    "@angular/localize": "^17.3.0",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "del-cli": "^5.1.0",
    "globby-cp": "^1.3.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "schematics-scss-migrate": "^2.3.17",
    "typescript": "~5.4.2"
  }
}
